{
  "realm": "hackathon",
  "enabled": true,
  "roles": {
    "realm": [
      { "name": "admin" },
      { "name": "judge" },
      { "name": "participant" }
    ]
  },
  "clients": [
    {
      "clientId": "hackathon-app",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": ["http://localhost:8080/*", "http://localhost:8762/*"],
      "webOrigins": ["*"],
      "directAccessGrantsEnabled": true,
      "standardFlowEnabled": true,
      "fullScopeAllowed": true,
      "defaultClientScopes": ["roles", "profile", "email"]
    }
  ]
}